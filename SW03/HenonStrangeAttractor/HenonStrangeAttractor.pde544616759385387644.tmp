//H: (x, y) -> (y, 1-ay^2+bx)

int startX;
int startY;
final int rectWidth = 50;
final int rectHeight= 50;
float a= 1.4;
float b = 0.3;
color colorRed;
int red = -65536;
int black = -16777216;



void setup() {
  size(800, 800);
  background(0);
  noStroke();
  startX= (width/2) - (rectWidth/2);
  startY= (width/2) - (rectHeight/2);
  draw50x50Points();
}


private void draw50x50Points() {
  fill(255, 0, 0);
  //Square in Middle of Screen
  rect(startX, startY, rectWidth, rectHeight);
  loadPixels();
  for (int x =0; x<width; x++) {
    for (int y = 0; y<height; y++) {
      int location = x+y*width;
      if (pixels[location]==color(255, 0, 0)) { 
        map(x, 0, width, -1.5, 1.5);
        map(y, 0, height, -0.4, 0.4);
        println('x:'+x);
        println('y:'+y)
        stroke(255, 0, 0);
        point(y, 1-a*y*y+b*x);
        noStroke();
      }
    }
  }
  updatePixels();
}


