void setup() {
  size(800, 800);
  background(0);
  stroke(255);
  drawEpizykloide(400, 400, 100, 40);
}

void drawEpizykloide(float x, float y, int outerRadius, int innerRadius) {
  point(x, y);
  float startX=x;
  float startY=y;
  float a= outerRadius;
  float b=innerRadius;


  for (float f = 0; f <100000; f++) {
    x =startX+(a+b)*cos(f) - a*cos((1+(b/a))*f);
    y=startY+(a+b)*sin(f) -a*sin((1+(b/a))*f);
    point(x,y);
  }
}