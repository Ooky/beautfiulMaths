void setup() {
 size(800, 800);
 background(0);
 drawSuperEllipse(400,400,400);
}


void drawSuperEllipse(int radius, int posX, int posY) {
  //Color
  stroke(255,255,255);
  //MiddlePoint
  point(posX, posY);
  
  int n = 1;
  int a = radius/2;
  int b = radius;
  for(float f = 0 ; f<720*2; f+=0.25) {//f<360; f+=1  //f<720; f+=0.5
   point(pow(cos(f), 2/n)*a,pow(sin(f), 2/n)*b); 
  }
  
 
  
}